#include<bits/stdc++.h>
using namespace std;
#define endl '\n'

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.setf(ios::fixed,ios::floatfield);

    int tc;

    cin>>tc;
    while(tc--)
    {
        int a1, a2, a3, a4;
        cin>>a1>>a2;
        cin>>a3>>a4;

        if(max(a1, a2) == max(a3, a4) && min(a1, a2)+min(a3, a4) == max(a1, a2))
            cout<<"Yes"<<endl;
        else
            cout<<"No"<<endl;
    }
    return 0;
}
